#!/usr/bin/expect -f
log_user 0
set timeout 2

proc check {input output} {
    send "$input\n"
    expect timeout {
	send_user "Failed: input \"$input\" did not result in \"$output\" \n"
	exit
    } "$output"
}

spawn ../../ccons
send "#include <math.h>\n"
check "pow(2.0, 4.0);" "=> (double) 16.0"
check "strcmp(\"abc\", \"def\");" \
      "error: implicit declaration of function 'strcmp'"
