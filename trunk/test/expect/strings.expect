#!/usr/bin/expect -f
log_user 0
set timeout 2

proc check {input output} {
    send "$input\n"
    expect timeout {
	send_user "Failed: input |$input| did not result in |$output| \n"
	exit
    } "$output"
}

spawn ../../ccons
check {"Hello World!";} {=> (char \[13\]) "Hello World!"}
send "char *x=\"Hi\";\n"
check {x;} {=> (char *) "Hi"}
send "char y\[3\]=\"Hi\";\n"
check {y;} {=> (char \[3\]) "Hi"}
